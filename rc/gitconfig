[alias]
    lg = !git --no-pager log --pretty='%C(yellow)%h %C(bold blue)%<(15,trunc)%an%Creset %Cgreen%ad%Creset %s%C(auto)%d%Creset' --graph --date=format:%H:%M -n32
    jeff = !git diff --ignore-space-at-eol -b -w --ignore-blank-lines
